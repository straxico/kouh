{
  "name": "back",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "author": "straxico",
  "license": "MIT",
  "scripts": {
    "start": "pnpm run serve",
    "build": " pnpm run build-ts",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\"  \"pnpm run watch-ts\" \"pnpm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "pnpm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "debug": "pnpm run build && pnpm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"pnpm run watch-ts\" \"pnpm run serve-debug\""
  },
  "dependencies": {
    "@types/agenda": "^3.0.2",
    "@types/mongoose-promise": "^4.5.10",
    "@types/socket.io-client": "^1.4.35",
    "agenda": "^4.0.1",
    "agendash": "^2.0.0",
    "async": "^3.2.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.5",
    "connect-mongo": "^4.1.0",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "hollaex-node-lib": "^1.2.0",
    "jsdom": "^16.4.0",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "mongoose": "^5.11.17",
    "socket.io-client": "^3.1.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/async": "^3.2.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.33",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.15",
    "@types/cheerio": "^0.22.24",
    "@types/concurrently": "^6.0.0",
    "@types/errorhandler": "^1.5.0",
    "@types/eslint": "^7.2.6",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsdom": "^16.2.6",
    "@types/lodash": "^4.14.168",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.31",
    "@types/request": "^2.48.5",
    "@types/request-promise": "^4.1.47",
    "@types/shelljs": "^0.8.8",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "chai": "^4.3.0",
    "concurrently": "^6.0.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "shelljs": "^0.8.5",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}